{
  "name": "git-permalink",
  "displayName": "git-permalink",
  "description": "Easy git permalinks",
  "version": "0.0.1",
  "publisher": "akashgajjar",
  "repository": "github:skywalker212/git-permalink",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-permalink.copyPermalink",
        "title": "Copy Permalink"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection && resourceScheme == 'file'",
          "command": "git-permalink.copyPermalink",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/node": "^20.19.7",
    "@types/sinon": "^17.0.4",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "sinon": "^21.0.0",
    "typescript": "^5.8.3"
  }
}